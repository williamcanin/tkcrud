{
    "Setup":{
        "app_name": "tkcrud",
        "app_version": "1.0.1",
        "app_license": "MIT Licences",
        "app_description": "Crud python using TKinter.",
        "app_url": "https://github.com/tkcrud/tkcrud",
        "author": {
            "name": "William da Costa Canin",
            "email": "william.costa.canin@gmail.com",
            "website": "https://williamcanin.me",
            "github": "https://github.com/williamcanin"
        },
        "app_classifiers": ["Programming Language :: Python :: 3",
            "License :: OSI Approved :: MIT License",
            "Operating System :: POSIX",
            "Operating System :: POSIX :: Linux",
            "Operating System :: MacOS :: MacOS X",
            "Operating System :: Unix"
        ],
        "python_requires": ">= 3.8",
        "platforms": "any",
        "console_scripts": [
            "tkcrud-run = tkcrud.tkcrud:TKCrud"
            ],
        "keywords": "tkcrud crud python mvc",
        "package_data": {
            "tkcrud": ["config/config.json", "data/schema.sql"]
        },
        "include_package_data": true
    }
}